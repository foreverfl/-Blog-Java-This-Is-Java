<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<HBox xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.DialogController" alignment="TOP_LEFT"
	spacing="10.0">
	<children>
		<Button onAction="#handleOpenFileChooser"
			text="Open FileChooser " />
		<Button onAction="#handleSaveFileChooser"
			text="Save FileChooser " />
		<Button onAction="#handleDirectoryChooser"
			text="DirectoryFileChooser " />
		<Button fx:id="btnPopup" onAction="#handlePopup" text="PopUp " />
		<Button fx:id="btnCustom" onAction="#handleCustom"
			text="Custom" />
	</children>
	<padding>
		<Insets top="10.0" right="10.0" bottom="10.0" left="10.0" />
	</padding>
</HBox>

