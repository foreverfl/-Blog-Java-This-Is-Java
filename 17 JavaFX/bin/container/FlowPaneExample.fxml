<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<FlowPane xmlns:fx="http://javafx.com/fxml/1" prefWidth="500" prefHeight="500">
	<padding>
		<Insets top="10.0" right="10.0" bottom="10.0" left="10.0" />
	</padding>
	<children>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: red"></VBox>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: orange"></VBox>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: yellow"></VBox>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: green"></VBox>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: blue"></VBox>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: navy"></VBox>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: purple"></VBox>
	</children>
</FlowPane>

