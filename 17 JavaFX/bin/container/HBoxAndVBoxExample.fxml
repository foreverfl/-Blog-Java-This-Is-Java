<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml/1">
	<padding>
		<Insets top="10.0" right="10.0" bottom="10.0" left="10.0" />
	</padding>
	<children>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: red"></VBox>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: orange"></VBox>
		<VBox prefHeight="100" prefWidth="100"
			style="-fx-background-color: yellow"></VBox>
		<HBox>
			<children>
				<HBox prefHeight="100" prefWidth="100"
					style="-fx-background-color: green"></HBox>
				<HBox prefHeight="100" prefWidth="100"
					style="-fx-background-color: blue"></HBox>
				<HBox prefHeight="100" prefWidth="100"
					style="-fx-background-color: navy"></HBox>
			</children>
		</HBox>
	</children>
</VBox>

